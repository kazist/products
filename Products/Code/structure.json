    "query": {
        "from": {
            "table": "Products\\Products\\Code\\Tables\\Products",
            "alias": "pp",
            "name": "#__products_products"
        },
        "select": [
            "pp.id",
            "pp.title",
            "pp.sku_code",
            "pp.category_id",
            "pc.title AS category_id_title",
            "pp.supplier_id",
            "ss.title AS supplier_id_title",
            "pp.description",
            "pp.price",
            "pp.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "pp.width",
            "pp.length",
            "pp.Height",
            "pp.weight",
            "pp.packaging",
            "pp.in_stock",
            "pp.except",
            "pp.published",
            "pp.created_by",
            "pp.date_created",
            "pp.modified_by",
            "pp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Products\\Categories\\Code\\Tables\\Categories",
                "alias": "pc",
                "name": "#__products_categories",
                "on": "pp.category_id = pc.id"
            },
            {
                "table": "Suppliers\\Suppliers\\Code\\Tables\\Suppliers",
                "alias": "ss",
                "name": "#__suppliers_suppliers",
                "on": "pp.supplier_id = ss.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "pp.image = mm.id"
            }
        ]
    }
